---
import Layout from '../layouts/Layout.astro';
import LandingHeader from '../components/LandingHeader.astro';
import HeroSection from '../components/HeroSection.astro';
import Model3 from '../components/Model3.astro';
import ModelX from '../components/ModelX.astro';
import ModelS from '../components/ModelS.astro';
import CyberTruck from '../components/CyberTruck.astro';
import Experience from '../components/Experience.astro';
import SolarPanel from '../components/SolarPanel.astro';
import SolarRoof from '../components/SolarRoof.astro';
import Powerwall from '../components/Powerwall.astro';
import Accessories from '../components/Accessories.astro';
---

<Layout title="Tesla">
	<LandingHeader />
	<main class="snap-y snap-mandatory relative w-full h-screen overflow-auto scroll-smooth">
		<div class="snap-center">
			<HeroSection />
		</div>
		<div class="snap-center">
			<Model3 />
		</div>
		<div class="snap-center">
			<ModelX />
		</div>
		<div class="snap-center">
			<ModelS />
		</div>
		<div class="snap-center">
			<CyberTruck />
		</div>
		<div class="snap-center">
			<Experience />
		</div>
		<div class="snap-center">
			<SolarPanel />
		</div>
		<div class="snap-center">
			<SolarRoof />
		</div>
		<div class="snap-center">
			<Powerwall />
		</div>
		<div class="snap-center">
			<Accessories />
		</div>
		
		
		
	</main>
</Layout>


